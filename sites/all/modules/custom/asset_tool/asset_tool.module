function asset_tool_conditions_load() {
dpm('haha');
dpm(module_implements('asset_tool_condition'));
  $conditions = array();
  foreach (	module_implements('asset_tool_condition') as $module) {
dpm($module);
    $func = $module . '_asset_tool_condition';
